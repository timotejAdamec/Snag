CREATE TABLE IF NOT EXISTS ProjectFailedSync (
    project_id TEXT NOT NULL,
    timestamp INTEGER NOT NULL
);

selectMostRecentFailedSync:
SELECT timestamp
FROM ProjectFailedSync
WHERE project_id = ?
ORDER BY timestamp DESC
LIMIT 1;

insertFailedSync:
INSERT OR REPLACE INTO ProjectFailedSync
VALUES ?;

deleteByProjectId:
DELETE FROM ProjectFailedSync
WHERE project_id = ?;

deleteAll:
DELETE FROM ProjectFailedSync;
